{
  "name": "pathplay",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "run-s lint",
    "lint": "run-p *:lint",
    "lint:staged": "lint-staged",
    "fix": "run-p *:fix",
    "js:lint": "eslint --quiet .",
    "js:fix": "eslint --fix .",
    "prettier:lint": "prettier --plugin-search-dir=. -c .",
    "prettier:fix": "prettier --plugin-search-dir=. --write .",
    "css:lint": "stylelint \"**/*.{css,scss,svelte}\"",
    "css:fix": "stylelint --fix \"**/*.{css,scss,svelte}\"",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{svelte,astro}": "stylelint",
    "**/*.{js,mjs,cjs,ts,astro,svelte}": "eslint -c .eslintrc.yml",
    "**/*": "prettier --plugin-search-dir=. -c"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "sass": "^1.58.3",
    "svelte": "npm:@typhonjs-svelte/svelte@3.55.1-cq2",
    "svelte-check": "^3.0.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "breakpoint-sass": "^3.0.0",
    "eslint": "^8.31.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "nanoid": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.2",
    "prettier-plugin-svelte": "^2.9.0",
    "sass-toolkit": "^2.10.2",
    "stylelint": "^14.0.0",
    "stylelint-config-prettier": "^9.0.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard": "^29.0.0"
  },
  "dependencies": {
    "idb": "^7.1.1",
    "breakpoint-sass": "^3.0.0",
    "sass-toolkit": "^2.10.2"
  },
  "volta": {
    "node": "18.14.2",
    "pnpm": "7.29.0"
  },
  "engines": {
    "node": "18.14.2"
  }
}
